<?xml version="1.0"?>
<rolemap>
  <roles>
    <role name="Elector" />
  </roles>
  <permissions>
    <permission
        name="collective.libreorganizacion: Add Proposal"
        acquire="True">
      <role name="Manager" />
      <role name="Elector" />
    </permission>
    <permission
        name="Reply to item"
        acquire="True">
      <role name="Manager" />
      <role name="Elector" />
    </permission>
    <permission
        name="Modify view template"
        acquire="False">
      <role name="Manager" />
      <role name="Editor" />
    </permission>
  </permissions>
</rolemap>
